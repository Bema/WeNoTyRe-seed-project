{
  "name": "wenotyre",
  "version": "1.0.0",
  "description": "Just another full-stack javascript seed project",
  "scripts": {
    "start": "./node_modules/.bin/npm-run-all dev",
    "build": "./node_modules/.bin/npm-run-all clean build:client build:server",
    "build:client": "NODE_ENV=production webpack --config ./webpack/webpack.config.js",
    "build:server_old": "NODE_ENV=production PORT=3000 ./node_modules/.bin/tsc -p ./src/server/tsconfig-server.json",
    "dev:client": "NODE_ENV=development PORT=3001 ./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.client.config.js -d --progress --no-info",
    "dev:server": "NODE_ENV=development PORT=3000 ./node_modules/.bin/webpack --config ./webpack/webpack.server.config.js --watch",
    "dev:server:run": "NODE_ENV=development PORT=3000 ./node_modules/.bin/nodemon ./build/server/index.js",
    "dev": "./node_modules/.bin/npm-run-all --parallel dev:client dev:server dev:server:run",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tslint:client": "./node_modules/.bin/tslint -p ./src/client/tsconfig-client.json --type-check -t verbose",
    "tslint:server": "./node_modules/.bin/tslint -p ./src/server/tsconfig-server.json --type-check -t verbode",
    "clean": "rm -rf build"
  },
  "keywords": [
    "react",
    "express",
    "typescript",
    "starter",
    "seed"
  ],
  "author": "bemacode",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^7.0.13",
    "@types/react": "^15.0.22",
    "@types/react-dom": "^15.5.0",
    "awesome-typescript-loader": "^3.1.2",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "html-webpack-plugin": "^2.28.0",
    "node-inspector": "^1.1.1",
    "nodemon": "^1.11.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "redux-immutable-state-invariant": "^2.0.0",
    "style-loader": "^0.16.1",
    "stylelint-webpack-plugin": "^0.7.0",
    "tslint": "^5.1.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-split-by-path": "^2.0.0"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0"
  }
}
