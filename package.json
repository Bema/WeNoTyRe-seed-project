{
  "name": "wenotyre",
  "version": "1.0.0",
  "description": "Just another full-stack javascript seed project",
  "scripts": {
    "start": "echo \"Please use start:client or start:server scripts\" && exit 1",
    "start:client": "npm run dev:client",
    "start:server": "npm run dev:server",
    "build": "./node_modules/.bin/npm-run-all build:clean build:client build:server",
    "build:clean": "rm -rf build",
    "build:client": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack/webpack.client.config.js",
    "build:server": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack/webpack.server.config.js",
    "dev": "echo \"Please use dev:client or dev:server scripts\" && exit 1",
    "dev:client": "NODE_ENV=development PORT=3001 ./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.client.config.js -d --progress --no-info",
    "dev:server": "./node_modules/.bin/npm-run-all build --parallel dev:server:watch dev:server:run",
    "dev:server:watch": "NODE_ENV=development PORT=3000 ./node_modules/.bin/webpack --config ./webpack/webpack.server.config.js --watch",
    "dev:server:run": "NODE_ENV=development PORT=3000 ./node_modules/.bin/forever -w ./build/server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "express",
    "typescript",
    "starter",
    "seed"
  ],
  "author": "bemacode",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^7.0.13",
    "@types/react": "^15.0.22",
    "@types/react-dom": "^15.5.0",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^3.1.2",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "cheerio": "^0.22.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "forever": "^0.15.3",
    "html-webpack-plugin": "^2.28.0",
    "node-inspector": "^1.1.1",
    "null-loader": "^0.1.1",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "redux-immutable-state-invariant": "^2.0.0",
    "style-loader": "^0.16.1",
    "stylelint-webpack-plugin": "^0.7.0",
    "tslint": "^5.1.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-split-by-path": "^2.0.0"
  },
  "dependencies": {
    "express": "^4.15.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0"
  }
}
